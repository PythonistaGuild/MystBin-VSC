{
  "name": "mystbin",
  "displayName": "MystBin",
  "description": "Easily share text and code with mystb.in",
  "version": "0.0.5",
  "license": "SEE LICENSE IN LICENSE",
  "publisher": "PythonistaGuild",
  "author": {
    "name": "PythonistaGuild",
    "url": "https://github.com/PythonistaGuild"
  },
  "icon": "images/icon.png",
  "galleryBanner": {
    "color": "#15151C",
    "theme": "dark"
  },
  "pricing": "Free",
  "engines": {
    "vscode": "^1.83.0"
  },
  "categories": [
    "Other"
  ],
  "keywords": [
    "paste",
    "share",
    "sharing"
  ],
  "activationEvents": [],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "mystbin.share",
        "title": "Share/Paste code to MystBin"
      }
    ],
    "menus": {
      "explorer/context": [
        {
          "command": "mystbin.share",
          "when": "resourceScheme == file"
        }
      ],
      "editor/context": [
        {
          "command": "mystbin.share",
          "when": "editorHasSelection || editorFocus"
        }
      ]
    }
  },
  "scripts": {
    "vscode:prepublish": "yarn run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "yarn run compile && yarn run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/mocha": "^10.0.2",
    "@types/node": "18.x",
    "@types/vscode": "^1.83.0",
    "@typescript-eslint/eslint-plugin": "^6.7.3",
    "@typescript-eslint/parser": "^6.7.3",
    "@vscode/test-electron": "^2.3.4",
    "eslint": "^8.50.0",
    "glob": "^10.3.3",
    "mocha": "^10.2.0",
    "typescript": "^5.2.2"
  },
  "dependencies": {
    "@types/node-fetch": "2.6.7",
    "node-fetch": "2.7.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/PythonistaGuild/MystBin-VSC"
  },
  "homepage": "https://github.com/PythonistaGuild/MystBin-VSC#readme"
}
